ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=conv2d_backward -DTORCH_API_INCLUDE_EXTENSION_H -isystem /home/akeller/anaconda3/envs/snf_icml/lib/python3.7/site-packages/torch/include -isystem /home/akeller/anaconda3/envs/snf_icml/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -isystem /home/akeller/anaconda3/envs/snf_icml/lib/python3.7/site-packages/torch/include/TH -isystem /home/akeller/anaconda3/envs/snf_icml/lib/python3.7/site-packages/torch/include/THC -isystem /home/akeller/anaconda3/envs/snf_icml/include/python3.7m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14
post_cflags = 
ldflags = -shared -L/home/akeller/anaconda3/envs/snf_icml/lib/python3.7/site-packages/torch/lib -lc10 -ltorch_cpu -ltorch -ltorch_python

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

build conv2d_backward.o: compile /home/akeller/repo/SNF_ICML/snf/utils/convbackward/conv2d_backward.cpp

build conv2d_backward.so: link conv2d_backward.o

default conv2d_backward.so

